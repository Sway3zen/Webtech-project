<!DOCTYPE html>
<html>

<head>
    <title>Admin Panel</title>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/styles/bootstrap.min.css">
    <link rel="stylesheet" href="/fonts/fonts.css">
    <link rel="stylesheet" href="/styles/admin.css">
    <link rel="stylesheet" href="/styles/header.css">
    <link rel="stylesheet" href="/styles/scroll.css">
</head>

<body>
  <nav class="navbar navbar-expand-xl navbar-light" style=" width:95%; z-index:2; margin-left:5%">
    <a class="navbar-brand" href="index.html">
      <img src="images/Logo.png" alt="Логотип" height="41" class="mt-0 navbar-logo">
      <div class="navbar-brand-name">
        <span class="navbar-brand-name-1">skylight</span>
        <span class="navbar-brand-name-2 text-uppercase text-decoration-none text-white"><br>media production agency</span>
      </div>
    </a>
  </nav>
    <div class="admin-box">
        <h1 class="container">SMPA Info panel</h1>
        <ul class="responsive-table container">
            <li class="table-header">
                <div class="col col-1">Name</div>
                <div class="col col-2">Phone</div>
                <div class="col col-3">Email</div>
                <div class="col col-4">Message</div>
            </li>
            <% feedbacks.forEach(function (feedback) { %>
                <li class="table-row">
                    <div class="col col-1" data-label="Job Id">
                        <%= feedback.name %>
                    </div>
                    <div class="col col-2" data-label="Customer Name">
                        <%= feedback.phone %>
                    </div>
                    <div class="col col-3" data-label="Amount">
                        <%= feedback.email %>
                    </div>
                    <div class="col col-4" data-label="Payment Status">
                        <%= feedback.message %>
                    </div>
                </li>
                <% }); %>

        </ul>
    </div>
</body>

</html>